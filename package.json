{
  "name": "nullvoid",
  "version": "2.1.0",
  "workspaces": [
    "packages/*",
    "ts",
    "js"
  ],
  "packageManager": "npm@11.6.2",
  "description": "Detect malicious code",
  "main": "ts/dist/scan.js",
  "types": "ts/dist/types/index.d.ts",
  "bin": {
    "nullvoid": "./ts/dist/bin/nullvoid.js"
  },
  "scripts": {
    "build": "turbo run build --filter=!@nullvoid/js --filter=!nullvoid-vscode",
    "build:watch": "cd ts && npm run build:watch",
    "dev": "cd ts && npm run dev",
    "test": "npm run test:js && npm run test:ts",
    "test:js": "cd js && npm test",
    "test:ts": "cd ts && npm test",
    "test:unit": "cd js && npm run test:unit",
    "test:integration": "cd js && npm run test:integration",
    "test:performance": "cd js && npm run test:performance",
    "test:watch": "cd js && npm run test:watch",
    "test:coverage": "cd js && npm run test:coverage",
    "scan": "node ts/dist/scan.js",
    "scan:dev": "cd ts && npm run scan:dev",
    "ml:serve": "cd ml-model && python3 serve.py --port 8000",
    "ml:train": "cd ml-model && python3 train.py --input train.jsonl --output model.pkl --balance --calibrate",
    "ml:dedup": "cd ml-model && node dedup-train.js",
    "ml:export": "cd ml-model && node export-features.js --out train.jsonl",
    "ml:export-behavioral": "cd ml-model && node export-behavioral-features.js --out train-behavioral.jsonl",
    "ml:train-behavioral": "cd ml-model && python3 train-behavioral.py --input train-behavioral.jsonl --balance --calibrate",
    "api:build": "turbo run build --filter=@nullvoid/api",
    "api:start": "turbo run build --filter=@nullvoid/api && node packages/api/dist/index.js",
    "dashboard:dev": "turbo run dev --filter=dashboard",
    "dashboard:build": "turbo run build --filter=dashboard",
    "ml:scan": "node ts/dist/bin/nullvoid.js scan",
    "badge": "node scripts/generate-badge.js",
    "badge:running": "node -e \"require('./scripts/generate-badge.js').setRunningBadge()\"",
    "badge:update": "npm run badge && git add README.md && git commit -m 'chore: Update dynamic test badge' || true",
    "lint": "cd ts && npm run lint",
    "lint:fix": "cd ts && npm run lint:fix",
    "format": "prettier --write \"ts/src/**/*.ts\"",
    "format:check": "prettier --check \"ts/src/**/*.ts\"",
    "type-check": "cd ts && npm run type-check",
    "prepare": "husky",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "security",
    "malware",
    "detection",
    "scanner",
    "javascript",
    "nodejs",
    "npm",
    "package",
    "supply-chain",
    "wallet-hijacking",
    "obfuscation",
    "sandbox",
    "static-analysis",
    "cli",
    "production-ready",
    "typescript"
  ],
  "author": "NullVoid Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.10.0"
  },
  "dependencies": {
    "@vercel/speed-insights": "^1.3.1",
    "async-mutex": "^0.5.0",
    "axios": "^1.13.5",
    "commander": "^11.1.0",
    "fs-extra": "^11.1.0",
    "glob": "^10.3.0",
    "js-yaml": "^4.1.1",
    "node-fetch": "^2.7.0",
    "ora": "^5.4.1",
    "tar": "^7.5.7"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@eslint/js": "^9.17.0",
    "@jest/globals": "^29.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.5.2",
    "@types/node-fetch": "^2.6.13",
    "@types/tar": "^6.1.13",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.4.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.0",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "ts/src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "overrides": {
    "js-yaml": "^4.1.1",
    "tar": "^7.5.7",
    "eslint": "^9.39.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kurt-grung/NullVoid.git"
  },
  "bugs": {
    "url": "https://github.com/kurt-grung/NullVoid/issues"
  },
  "homepage": "https://github.com/kurt-grung/NullVoid#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "src/",
    "bin/",
    "lib/",
    "ts/",
    "scan.js",
    "colors.js",
    "README.md",
    "LICENSE",
    "tsconfig.json"
  ]
}
