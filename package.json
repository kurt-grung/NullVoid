{
  "name": "nullvoid",
  "version": "1.3.4",
  "description": "Detect and invalidate malicious npm packages before they reach prod",
  "main": "index.js",
  "bin": {
    "nullvoid": "./bin/nullvoid.js"
  },
  "scripts": {
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:performance": "jest test/performance",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "scan": "node scan.js"
  },
  "keywords": [
    "npm",
    "security",
    "malware",
    "package",
    "scan",
    "detection",
    "cli"
  ],
  "author": "NullVoid Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "acorn": "^8.10.0",
    "acorn-walk": "^8.2.0",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.1.0",
    "glob": "^10.3.0",
    "node-fetch": "^2.7.0",
    "ora": "^5.4.1",
    "tar": "^6.2.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kurt-grung/NullVoid.git"
  },
  "bugs": {
    "url": "https://github.com/kurt-grung/NullVoid/issues"
  },
  "homepage": "https://github.com/kurt-grung/NullVoid#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin/",
    "scan.js",
    "README.md",
    "LICENSE"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "scan.js",
      "bin/nullvoid.js",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ]
  }
}
