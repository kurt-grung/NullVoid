{
  "name": "nullvoid-vscode",
  "displayName": "NullVoid Security Scanner",
  "description": "Run NullVoid security scans from VS Code",
  "version": "0.1.0",
  "publisher": "nullvoid",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Testing"
  ],
  "activationEvents": [
    "onCommand:nullvoid.runScan"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nullvoid.runScan",
        "title": "NullVoid: Run Security Scan"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -watch -p ."
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.74.0",
    "typescript": "^5.0.0"
  }
}
